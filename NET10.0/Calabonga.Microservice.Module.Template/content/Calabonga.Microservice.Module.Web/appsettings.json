{
    "Cors": {
        "Origins": "*"
    },
    "Serilog": {
        "Using": [ "Serilog.Sinks.Console" ],
        "MinimumLevel": {
            "Default": "Verbose",
            "Override": {
                "Microsoft": "Debug",
                "System": "Debug"
            }
        },
        "WriteTo": [
            { "Name": "Console" }
        ],
        "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
    },
    "AuthServer": {
        "Url": "https://localhost:10001"
    },
    "AllowedHosts": "*"
}
